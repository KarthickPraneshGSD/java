//w/o arg & return

import java.util.*;
class BE
{
	static void sum()
	{
		int a=5;
		int b=6;
		int c=a+b;
		System.out.println(c);
	}
	public static void main(String[] args)
	{
		sum();
	}
}

//with arg & return

import java.util.*;
class BE
{
	static int sum(int x,int y)
	{
	    int z=x+y;
	    return z;
	    
	}
	    public static void main(String[] args) 
	    {
	        int a=5;
	        int b=6;
	        int c=sum(a,b);
	        System.out.println(c);
	    }
	}

	//with arg w/o return

import java.util.*;
public class BE
{
	static void sum(int a,int b){
	    System.out.println(a+b);}
	    public static void main(String[] args) {
	        int x=5;
	        int y=6;
	        sum(x,y);
	    }
	}

	//w/o arg with return

import java.util.*;
public class Main
{
	static int sum(){
	    int a=5;
	    int b=6;
	    int c=a+b;
	    return c;
	    }
	    public static void main(String[] args) {
	        int result=sum();
	        System.out.println(result);
	        System.out.println(sum());
	    }
	}

	
