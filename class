import java.util.*;
public class Person
    {
        int age;
        String name;
        Person(String name,int age)
        {
        this.name=name;
        this.age=age;
    }
    void display()
    {
        System.out.println(name);
        System.out.println(age);
    }
	public static void main(String[] args) 
	{
	 Person p1=new Person("GSD",20);
	 p1.display();
	}
}
