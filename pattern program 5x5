import java.util.*;
public class Main
{
	public static void main(String[] args)
	{
	    Scanner n = new Scanner(System.in);
	    int a = n.nextInt();
	    for (int i=1; i<=a; i++ )
	    {
	        for (int j=1;j<=a;j++)
	        {
	            System.out.print("* ");
	        }
	        	System.out.println();
	    }
	}
}
